{
  "name": "my-extension",
  "description": "my extension",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["loader.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.global.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "scripting",
    "tabs"
  ]
}
